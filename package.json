{
    "name": "vscode-todo-timer",
    "displayName": "vscode-todo-timer",
    "description": "",
    "version": "0.0.1",
    "publisher": "shushutona",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:timer-controller"
    ],
    "main": "./out/extension",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "todo-timer",
                    "title": "ToDo Timer",
                    "icon": "./out/img/icon.svg"
                }
            ]
        },
        "views": {
            "todo-timer": [
                {
                    "id": "timer-controller",
                    "name": "Timer Controller"
                },
                {
                    "id": "todo-list",
                    "name": "ToDo List"
                }
            ]
        },
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "hello from json",
                "category": "Hello"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
